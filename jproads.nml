		param {
			param_intro_year {
				type:      int;
				name:      string(STR_PARAM_1_NAME);
				desc:      string(STR_PARAM_1_DESC);
				min_value: 0;
				max_value: 1;
				def_value: 0;
				names: {
					0: string(STR_PARAM_INTRO_YEAR_DEF);
					1: string(STR_PARAM_INTRO_YEAR_REMOVE);
				};
			}
